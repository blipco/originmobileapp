{
  "expo": {
    "name": "origin-mobile",
    "description": "A very interesting project.",
    "slug": "origin-mobile",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": [ "ios","android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/apple-oca-app-152x152.png",
    "splash": {
      "image": "./assets/images/OCAvertical.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oca.OCApp"
    },
    "android": {
      "package": "com.oca.OCApp",
      "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "mdpi": "./assets/images/oca_app_mdpi.png",
        "hdpi": "./assets/images/oca_app_hdpi.png",
        "xhdpi": "./assets/images/oca_app_xhdpi.png",
        "xxhdpi": "./assets/images/oca_app_xxhdpi.png",
        "xxxhdpi": "./assets/images/oca_app_xxxhdpi.png"
      }
    }
  }
}